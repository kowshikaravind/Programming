#include <stdio.h>
#include <string.h>
void repeating(char *s){
	char a[100];
	int count[256]={0};
	int flag=0;
	for(int i=0;i<strlen(s);i++){
		count[s[i]]++;
		if(count[s[i]]==2){
			printf("%c",s[i]);
			flag=1;
			return;
		}
	}
	if(flag==0){
		printf("No repeating character");
	}
}
int main(){
	char s[100];
	scanf("%[^\n]",s);
	repeating(s);
	
}
